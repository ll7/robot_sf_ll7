{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/robot-sf/resume-manifest.schema.v1.json",
    "title": "RobotSF Resume Manifest (v1)",
    "type": "object",
    "required": [
        "version",
        "episodes"
    ],
    "additionalProperties": false,
    "properties": {
        "version": {
            "const": "v1"
        },
        "episodes": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "meta": {
            "type": "object",
            "additionalProperties": true
        }
    }
}